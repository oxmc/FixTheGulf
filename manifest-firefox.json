{
  "manifest_version": 2,
  "name": "Restore Gulf of Mexico",
  "version": "1.0",
  "description": "Updates Google Maps to show the Gulf of Mexico again.",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/maps/*"
      ],
      "js": [
        "gulf.js"
      ],
      "run_at": "document_idle"
    }
  ]
}